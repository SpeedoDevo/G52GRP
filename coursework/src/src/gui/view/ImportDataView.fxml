<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="500.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.controller.ImportDataController">
   <children>
      <Label alignment="CENTER" layoutX="19.0" layoutY="14.0" prefHeight="73.0" prefWidth="700.0" text="Import Data" textAlignment="CENTER" AnchorPane.leftAnchor="50.0" AnchorPane.rightAnchor="50.0">
         <font>
            <Font size="50.0" />
         </font>
      </Label>
      <VBox alignment="CENTER" fillWidth="false" layoutX="221.0" layoutY="113.0" prefHeight="232.0" prefWidth="600.0" spacing="20.0" AnchorPane.leftAnchor="50.0" AnchorPane.rightAnchor="50.0">
         <children>
            <Label text="Choose delimiter">
               <graphic>
                  <TextField fx:id="delimeter" onKeyReleased="#delimeterFieldChange" promptText="Type delimeter" />
               </graphic>
            </Label>
            <Button fx:id="bSelectFile" layoutX="221.0" layoutY="164.0" mnemonicParsing="false" onAction="#loadFile" text="Select File">
               <font>
                  <Font size="27.0" />
               </font>
            </Button>
            <Button layoutX="326.0" layoutY="323.0" mnemonicParsing="false" text="Example Data" onAction="#exampleData">
               <font>
                  <Font size="27.0" />
               </font>
            </Button>
         </children>
      </VBox>
   </children>
</AnchorPane>
