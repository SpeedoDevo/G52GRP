<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?scenebuilder-stylesheet vista.css?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>

<VBox prefHeight="500.0" prefWidth="800.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8" fx:controller="gui.controller.MainController">
	<children>
		<StackPane fx:id="vistaHolder" VBox.vgrow="ALWAYS" />
		<ToolBar prefHeight="40.0" prefWidth="318.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <items>
   			<Button fx:id="back" onAction="#back" text="Back" />
   			<Button onAction="#showHelp" text="Help" />
   			<Pane HBox.hgrow="ALWAYS" />
   			<CheckBox fx:id="advanced" onAction="#advancedChanged" text="Toggle Advanced Mode" />
   </items>
		</ToolBar>
  </children>
</VBox>
